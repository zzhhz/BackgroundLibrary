<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:state_pressed="true" >
        <layer-list>
            <item>
                <shape
                    android:shape="rectangle"
                    >
                    <corners
                        android:radius="3dp"
                        />
                    <solid
                        android:color="#9DD1F6"
                        />
                    <stroke
                        android:width="1dp"

                        android:color="#9DD1F6" />
                </shape>
            </item>
        </layer-list>
    </item>




    <item android:state_enabled="true">
        <layer-list>
            <item>
                <shape
                    android:shape="rectangle"
                    >
                    <corners
                        android:radius="3dp"
                        />
                    <solid
                        android:color="#1B82D1"
                        />
                    <stroke
                        android:width="1dp"

                        android:color="#1B82D1" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_enabled="false" >
        <layer-list>
            <item>
                <shape
                    android:shape="rectangle"
                    >
                    <corners
                        android:radius="3dp"
                        />
                    <solid
                        android:color="#9DD1F6"
                        />
                    <stroke
                        android:width="1dp"

                        android:color="#9DD1F6" />
                </shape>
            </item>
        </layer-list>
    </item>

</selector>




